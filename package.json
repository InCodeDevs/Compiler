  "bugs": {
    "url": "https://github.com/InCodeDevs/Compiler/issues"
  },
  "homepage": "https://github.com/InCodeDevs/Compiler#readme",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^16.11.10",
    "chalk": "^4.0.0",
    "commander": "^8.3.0",
    "copyfiles": "^2.4.1",
    "typescript": "^4.5.2",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "prettier": "^2.4.1"
  }
